                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <!-- Repeat the pattern for the rest of the days -->
                        </tr>
                    </thead>
                    <!--START THE SCHEDULE -->
                    <tbody class="scroll-content">
                        {{#each scheduleData}}
                        <tr>
                            {{#each this}}
                            {{!-- <td>{{this}}</td> --}}
                            <td>{{#if this}}{{this}}{{else}}&nbsp;{{/if}}</td>
                            {{/each}}
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>