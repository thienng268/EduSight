                        <th colspan="5">Chiều</th>
                        <th colspan="5">Sáng</th>
                        <th colspan="5">Chiều</th>
                        </tr>
                        <tr>
                        <!-- The third row headers -->
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
                        <th>Tiết 1</th>
                        <th>Tiết 2</th>
                        <th>Tiết 3</th>
                        <th>Tiết 4</th>
                        <th>Tiết 5</th>
